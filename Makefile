NAME 		= push_swap
CC 			= cc
CFLAGS 		= -g -Wall -Wextra -Werror 
RM 			= rm -f
INC 		= -I./libft/includes -I.
LIBFTPATH 	= ./libft
SRC 		= 

all: $(NAME)

$(NAME): $(SRC)
	$(MAKE) -C $(LIBFTPATH)
	$(CC) $(CFLAGS) $(INC) $(SRC) -Llibft -lft -o $(NAME)
	@$(MAKE) luna

        
clean:
	$(MAKE) clean -C $(LIBFTPATH)

fclean: clean
	$(MAKE) fclean -C $(LIBFTPATH)
	
	$(RM) $(NAME)

re: fclean all

.PHONY: all clean fclean re
.SILENT: 

luna:
